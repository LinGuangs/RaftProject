{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Raft",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/raft_node",  // 可执行文件路径
            "args": [],                     // 程序参数，如有需要可以填写
            "cwd": "${workspaceFolder}",    // 工作目录
            "stopAtEntry": false,
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
